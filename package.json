{
  "name": "vnoriega-dev",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prettier": "prettier --write \"src/**/*.{js,ts,vue,scss}\"",
    "prettier:check": "prettier --check \"src/**/*.{js,ts,vue,scss}\"",
    "dev:host": "vite -- --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/tsconfig": "^0.7.0",
    "@testing-library/vue": "^8.0.1",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.5.3",
    "sass": "^1.83.4",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vue-tsc": "^2.2.0"
  }
}
